{
  "name": "games-table",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "setup-env": "$(npm bin)/ts-node ./environments/script/env-setup.ts",
    "start": "npm run setup-env & ng serve",
    "build-micro": "ng build --prod --single-bundle",
    "build-micro-live": "ng build --watch=true --optimization=false  --single-bundle",
    "start-micro": "npm run setup-env & http-server ./dist-micro -p 8001",
    "start-micro-live": "npm run setup-env & live-server ./dist-micro --port=8001 --host=localhost --no-browser",
    "start-playground": "concurrently \"npm run build-micro-live\" \"npm run start-micro-live\" \"npm run setup-env\" \"cd ../micro-ui-demo && npm start\" ",
    "start-playground-ci": "env ENV=ci concurrently \"npm run build-micro-live\" \"npm run start-micro-live\" \"npm run setup-env\" \"cd ../micro-ui-demo && npm start\" ",
    "start-playground-stg": "env ENV=stg concurrently \"npm run build-micro-live\" \"npm run start-micro-live\" \"npm run setup-env\" \"cd ../micro-ui-demo && npm start\" ",
    "test": "ts-patch install && npm run setup-env & ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "test-local": "ts-patch install && npm run setup-env & node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng test --watch=true --code-coverage --browsers Chrome",
    "test-local-headless": "ts-patch install && npm run setup-env & node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng test --watch=true --code-coverage --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier 'src/**/*.ts' --write"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.0",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/elements": "^8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^6.4.7",
    "@angular/material-moment-adapter": "^8.2.0",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.0",
    "concurrently": "^5.2.0",
    "document-register-element": "^1.7.2",
    "http-server": "^0.11.1",
    "karma-junit-reporter": "^2.0.1",
    "live-server": "^1.2.1",
    "lodash": "^4.5.0",
    "moment": "^2.24.0",
    "ng-lottie": "^0.3.2",
    "ngx-build-plus": "^9.0.4",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "uuid": "^8.1.0",
    "winston": "3.3.0",
    "yaml": "^1.7.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.1",
    "@angular/cli": "~8.2.1",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@netbasal/spectator": "^3.9.4",
    "@ngneat/spectator": "6.1.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "8.10.52",
    "codelyzer": "^5.0.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-junit-reporter": "^2.0.1",
    "lite-server": "^2.5.4",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "protractor": "~5.4.0",
    "ts-auto-mock": "^2.2.7",
    "ts-auto-mock-random": "^1.5.5",
    "ts-node": "~7.0.0",
    "ts-patch": "^1.2.4",
    "tslint": "~5.15.0",
    "typescript": "~3.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
